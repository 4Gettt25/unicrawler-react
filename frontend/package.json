{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.14.5",
		"@supabase/supabase-js": "^2.44.4",
		"@testing-library/user-event": "^13.5.0",
		"axios": "^1.7.2",
		"bootstrap": "^5.3.3",
